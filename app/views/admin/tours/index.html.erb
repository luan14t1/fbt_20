<% provide :title, t(".title") %>
<% provide :subject, t(".subject") %>
  <div class="panel-body">
    <%= link_to (t".add"), "#", class: "btn btn-info" %>
    <div class="table-responsive">
      <table class="table table-striped table-bordered table-hover" id="dataTables-example">
        <thead>
          <tr>
            <th><%= t".number" %></th>
            <th><%= t".name" %></th>
            <th><%= t".description" %></th>
            <th><%= t".image" %></th>
            <th><%= t".number_user" %></th>
            <th><%= t".start_date" %></th>
            <th><%= t".end_date" %></th>
            <th><%= t".price" %></th>
            <th><%= t".rating" %></th>
            <th><%= t".category" %></th>
            <th><%= t".function" %></th>
          </tr>
        </thead>
        <tbody>
          <%= render @tours %>
        </tbody>
      </table>
    </div>
  </div>
